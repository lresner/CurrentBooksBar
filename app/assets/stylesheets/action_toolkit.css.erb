.zipcode-form {
  display: none;
}

#map-canvas {
  height: 100%;
  width: 100%;
  margin-top: 110px;
  margin: auto;
  padding-top: 10px;
  position:absolute;
  left: 0;
  bottom: 0;
}

.orgsWrapper {
  height: 100px;
  padding: 10px 10px 0 10px;
  z-index: 99;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  background-color: #464646;
  color: white;
}

.orgsWrapper h1 {
  margin-top: 40px;
}

.rerenderMap {
  position: absolute;
  top: 10px;
  left: 10px;
}

.gm-style-iw {
  max-height:100%;
  overflow-y:auto;
}